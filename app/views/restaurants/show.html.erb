<div class="container">
<h2><%=@restaurant.name%></h2>
<p><%= "#{@restaurant.address},#{@restaurant.phone_number}. Category :#{@restaurant.category}" %></p>


  <% @restaurant.reviews.each do |review| %>
    <p><%= review.rating %></p>
    <p><%= review.content %></p>
  <% end %>
<p><%= link_to "Add new review",new_restaurant_review_path(@restaurant) %></p>
<p><%= link_to "Go back", root_path %></p>
</div>
